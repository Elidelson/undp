{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "undpopendata-df"
		},
		"db_phl_opr_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'db_phl_opr'"
		},
		"db_phl_prog_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'db_phl_prog'"
		},
		"undpdigitalml_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "1c766f37-8740-4764-932c-bf96cbb29a39"
		},
		"undpdigitalml_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "undpdigitalml"
		},
		"undpdigitalml_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "814c3c10-455e-4b6f-935b-0554d5f32db0"
		},
		"undpdigitalml_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "b3e5db5e-2944-4837-99f5-7488ace54319"
		},
		"undpdigitalopendata_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://undpdigitalopendata.vault.azure.net/"
		},
		"undpdigitalopendatastore_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "undpdigitalopendatastorage"
		},
		"phl_opr_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1c766f37-8740-4764-932c-bf96cbb29a39/resourceGroups/undpdipdata/providers/Microsoft.Storage/storageAccounts/undpdigitalopendatastore"
		},
		"phl_prog_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1c766f37-8740-4764-932c-bf96cbb29a39/resourceGroups/undpdipdata/providers/Microsoft.Storage/storageAccounts/undpdigitalopendatastore"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"db_phl_opr_connectionString": {
						"value": "[parameters('db_phl_opr_connectionString')]"
					},
					"db_phl_prog_connectionString": {
						"value": "[parameters('db_phl_prog_connectionString')]"
					},
					"undpdigitalopendata_properties_typeProperties_baseUrl": {
						"value": "[parameters('undpdigitalopendata_properties_typeProperties_baseUrl')]"
					},
					"phl_opr_trigger_properties_typeProperties_scope": {
						"value": "[parameters('phl_opr_trigger_properties_typeProperties_scope')]"
					},
					"phl_prog_trigger_properties_typeProperties_scope": {
						"value": "[parameters('phl_prog_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
