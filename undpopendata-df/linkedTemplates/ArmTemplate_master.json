						"value": "[parameters('undpdigitalml_properties_typeProperties_resourceGroupName')]"
					},
					"undpdigitalml_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('undpdigitalml_properties_typeProperties_servicePrincipalId')]"
					},
					"undpdigitalml_properties_typeProperties_tenant": {
						"value": "[parameters('undpdigitalml_properties_typeProperties_tenant')]"
					},
					"undpdigitalopendatastore_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('undpdigitalopendatastore_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
