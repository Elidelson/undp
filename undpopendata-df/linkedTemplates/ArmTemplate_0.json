{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "undpopendata-df"
		},
		"db_phl_opr_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'db_phl_opr'"
		},
		"db_phl_prog_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'db_phl_prog'"
		},
		"phl_sdgourbiz_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'phl_sdgourbiz'"
		},
		"undpdigitalopendata_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://undpdigitalopendata.vault.azure.net/"
		},
		"PHL-NewBlob_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1c766f37-8740-4764-932c-bf96cbb29a39/resourceGroups/undpdipdata/providers/Microsoft.Storage/storageAccounts/undpdigitalopendatastore"
		},
		"phl_sdgourbiz_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "67781619-c891-4826-9c79-8286ae9c01fa"
		},
		"phl_sdgourbiz_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "b3e5db5e-2944-4837-99f5-7488ace54319"
		},
		"undpdigitalopendatastore_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "undpdigitalopendatastorage"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/db_phl_opr')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('db_phl_opr_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/db_phl_prog')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('db_phl_prog_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/undpdigitalopendata')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('undpdigitalopendata_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/PHL-NewBlob')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/opendataphl/blobs/",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('PHL-NewBlob_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/phl_prog_sdg_dashboard_tbl')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "db_phl_prog",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbo",
					"table": "sdg_dashboard"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/db_phl_prog')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/phl_sdgourbiz')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('phl_sdgourbiz_connectionString')]",
					"servicePrincipalId": "[parameters('phl_sdgourbiz_properties_typeProperties_servicePrincipalId')]",
					"servicePrincipalKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "undpdigitalopendata",
							"type": "LinkedServiceReference"
						},
						"secretName": "spphldbadmin"
					},
					"tenant": "[parameters('phl_sdgourbiz_properties_typeProperties_tenant')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/undpdigitalopendata')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/undpdigitalopendatastore')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "undpdigitalopendata",
							"type": "LinkedServiceReference"
						},
						"secretName": "[parameters('undpdigitalopendatastore_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/undpdigitalopendata')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/phl_prog_sdg_dashboard_csv')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "undpdigitalopendatastore",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "undpph_sdg_dashboard_data.csv",
						"folderPath": "Shared Documents/Technical/Programme Data/SDG Dashboard",
						"container": "phl-prog"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "REG_PSGC",
						"type": "String"
					},
					{
						"name": "REGION",
						"type": "String"
					},
					{
						"name": "PROV_PSGC",
						"type": "String"
					},
					{
						"name": "PROVINCE",
						"type": "String"
					},
					{
						"name": "MUNCITY_PSGC",
						"type": "String"
					},
					{
						"name": "MUNCITY_NAME",
						"type": "String"
					},
					{
						"name": "variable",
						"type": "String"
					},
					{
						"name": "value",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/undpdigitalopendatastore')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/phl_prog')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy SDG Dashboard",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "AzureSqlSink",
								"tableOption": "autoCreate"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "REG_PSGC",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "REG_PSGC",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "REGION",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "REGION",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "PROV_PSGC",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "PROV_PSGC",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "PROVINCE",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "PROVINCE",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "MUNCITY_PSGC",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "MUNCITY_PSGC",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "MUNCITY_NAME",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "MUNCITY_NAME",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "variable",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "variable",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "value",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "value",
											"type": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "phl_prog_sdg_dashboard_csv",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "phl_prog_sdg_dashboard_tbl",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/phl_prog_sdg_dashboard_csv')]",
				"[concat(variables('factoryId'), '/datasets/phl_prog_sdg_dashboard_tbl')]"
			]
		}
	]
}